{
  "name": "@jrsmiffy/delaunator",
  "private": false,
  "version": "1.4.3",
  "description": "TypeScript library that generates Delaunay Triangulations",
  "main": "delaunay.js",
  "scripts": {
    "test": "jest --config jest.config.js",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrsmth/delaunator.git"
  },
  "keywords": [
    "Delaunay",
    "Triangulation"
  ],
  "author": "J. R. Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrsmth/delaunator/issues"
  },
  "homepage": "https://github.com/jrsmth/delaunator#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ]
}
